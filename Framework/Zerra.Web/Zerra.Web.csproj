<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net5.0;net6.0</TargetFrameworks>
      <Authors>Steven Zawaski</Authors>
      <Copyright>KaKush LLC</Copyright>
      <Product>Zerra Framework Repository Sql</Product>
      <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
      <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <Description>Web and AspNetCore specific components for the Zerra Framework.

Commonly used types:
Zerra.Web.CQRSMiddleware
Zerra.Web.ZerraLogger
Zerra.Web.CheckBoxList</Description>
    <RepositoryUrl>https://github.com/szawaski/Zerra</RepositoryUrl>
    <PackageTags>Framework, CQRS, Microservice, Event Sourcing, Repository, ORM, ASPNET, Web</PackageTags>
    <LangVersion>10.0</LangVersion>
    <Version>4.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.ViewFeatures" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Zerra\Zerra.csproj" />
  </ItemGroup>

</Project>
