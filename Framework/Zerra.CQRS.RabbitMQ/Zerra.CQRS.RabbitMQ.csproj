<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net5.0;net6.0</TargetFrameworks>
    <Authors>Steven Zawaski</Authors>
    <Copyright>KaKush LLC</Copyright>
    <Product>Zerra Framework CQRS Rabbit Message</Product>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <LangVersion>8.0</LangVersion>
    <Description>RabbitMQ implementations of producer and consumer for commands and events in the CQRS Zerra Framework. 

Commonly used types:
Zerra.CQRS.RabbitMQ.RabbitMQClient
Zerra.CQRS.RabbitMQ.RabbitMQServer</Description>
    <Company>KaKush LLC</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>Framework, CQRS, Microservice, Event Sourcing, Repository, ORM, RabbitMQ</PackageTags>
    <RepositoryUrl>https://github.com/szawaski/Zerra</RepositoryUrl>
	<Version>3.4.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Zerra\Zerra.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RabbitMQ.Client" Version="5.1.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
